jQuery(document).ready(function ($) {

    let discountCodeid = $('.discountCodeid .elementor-heading-title');
    $(discountCodeid).addClass('couponCode');

    let clipboard = new ClipboardJS('.copyDiscountCode', {
    target: function () {
        return document.querySelector('.couponCode');
    },
});

    clipboard.on('success', function (e) {
        $('.copiedToolTip').fadeIn(200).delay(1000).fadeOut(200);
        e.clearSelection();
    });

});